# Vercel ignore file
# Files and directories to exclude from Vercel deployments

# Development files
.env.local
.env.development
.env.test

# Test files
tests/
**/*.test.ts
**/*.spec.ts
playwright-report/
test-results/

# Documentation (optional - remove if you want docs deployed)
docs/

# Build artifacts that shouldn't be in source
dist/
*.min.js
*.min.css

# IDE files
.vscode/
.idea/
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Logs
*.log
npm-debug.log*

# Storybook
.storybook/
storybook-static/

# AI agent skills (development tooling)
.agents/
